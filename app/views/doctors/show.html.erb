<section id="doctor-info">
  <h1>Doctor <%= @doctor.name %></h1>
  <p>specialty: <%= @doctor.specialty %></p>
  <p>University: <%= @doctor.university %></p>
  <p>Hospital: <%= @doctor.hospital.name %></p>
</section>
<section id="patients">
  <p>Doctors's Patiens</p>
  <% @doctor.patients.each do |patient| %>
  <section id="patient-<%= patient.id %>">
    <p>Name: <%= patient.name %></p>
    <%= button_to 'Remove Patient', "/doctors/#{@doctor.id}/#{patient.id}", method: :delete %>
  </section>
  <% end %>
</section>
